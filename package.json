{
  "name": "receipt-server",
  "author": "Ynewtime",
  "version": "0.5.2",
  "bin": {
    "receipt-server": "bin/receipt-server.mjs",
    "receipt-net": "bin/receipt-net.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "nodemon -w src -w lib -w test src/index.mjs",
    "dev:net": "nodemon -w src -w lib -w test src/receipt-net.mjs",
    "start": "node src/index.mjs",
    "start:net": "node src/receipt-net.mjs",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "numeral": "^2.0.6",
    "ping": "^0.4.2",
    "usb": "^2.5.2"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "nodemon": "^2.0.20",
    "unbuild": "^0.9.4"
  }
}
